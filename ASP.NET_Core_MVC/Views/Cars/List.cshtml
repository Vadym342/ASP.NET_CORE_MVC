<h2>All cars</h2>
<h3>@Model.currCategory</h3>
    @{ 
        foreach(var car in Model.AllCars)
        {
            <div>
                <h2>Name: @car.name</h2>
                <p>
                    Price: @car.price.ToString("c")

                </p>


            </div>
        }
    }



